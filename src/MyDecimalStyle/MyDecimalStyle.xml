<?xml version="1.0" encoding="utf-8" ?>
<widget id="MyDecimalStyle.widget.MyDecimalStyle" needsEntityContext="true" 
    xmlns="http://www.mendix.com/widget/1.0/">
    <name>MyDecimalStyle</name>
    <description>The description of this widget.</description>
    <icon></icon>
    <properties>
        <!-- <property key="dummyKey" type="string" required="false" defaultValue=""><caption>Dummy key</caption><category>Appearance</category><description>This key was added to make the widget work in the Modeler (needs atleast 1 key). Please change/remove this one</description></property> -->
        <property key="field" type="attribute" required="true" isPath="optional" pathType="reference">
            <caption>Field</caption>
            <category>General</category>
            <description>Field to Render</description>
            <attributeTypes>
                <attributeType name="Decimal" />
            </attributeTypes>
        </property>
        <property key="beforeClassname" type="string" required="false" defaultValue="">
            <caption>Before Classname</caption>
            <category>General</category>
            <description></description>
        </property>
        <property key="afterClassname" type="string" required="false" defaultValue="">
            <caption>After Classname</caption>
            <category>General</category>
            <description></description>
        </property>
        <!-- This is just a holder for the type of entity returned from the mf -->
        <property key="returnEntity" type="entity" isPath="optional" allowNonPersistableEntities="true" pathType="reference">
            <caption>Return Entity</caption>
            <category>Behavior</category>
            <description>The Entity returned by the (`On Click`) property.</description>
        </property>
        <property key="onClickMicroflow" type="microflow" required="false">
            <caption>On Click</caption>
            <category>Behavior</category>
            <description>This microflow will be called when the user clicks on the widget.</description>
            <returnType type="Object" entityProperty="returnEntity"></returnType>
        </property>
    </properties>
</widget>
